[Desktop Entry]
Type=Application
Name=@APP_NAME@
Version=@VERSION@
Comment=@COMMENT@
GenericName=@GENERIC_NAME@
StartupWMClass=@EXEC_NAME@
Exec=@EXEC_NAME@
Icon=@ICON_NAME@
Terminal=false
Categories=GTK;Utility;Productivity
Keywords=Note;Notes;Write;Keep;
X-GNOME-Keywords=Note;Notes;Write;Keep;
StartupNotify=true
X-GNOME-Gettext-Domain=@GETTEXT_PACKAGE@
Actions=AboutDialog;

[Desktop Action AboutDialog]
Exec=keep --about
Name=About Keep
